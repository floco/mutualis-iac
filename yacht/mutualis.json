  {
    "type": 1,
    "title": "Adguardhome",
    "name": "adguardhome",
    "description": "Ad blocker like PIHole",
    "categories": [
      "DNS"
    ],
    "platform": "linux",
    "logo": "https://developer.asustor.com/uploadIcons/0020_999_1595573041_AdGuardhome_256.png",
    "image": "adguard/adguardhome",
    "restart_policy": "unless-stopped",
    "env": [
      {
        "name": "PUID",
        "label": "PUID",
        "default": "!PUID"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "!PGID"
      }
    ],
    "ports": [
      {
        "dnstcp": "53:53/tcp",
